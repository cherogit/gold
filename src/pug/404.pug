include mixins/_breadcrumbs.pug
include mixins/_accodrion-list.pug

doctype html
html(lang="ru")
	include includes/head.pug
	body
		include includes/preloader.pug
		include includes/header.pug

		main.main
			.not-found
				+breadcrumbs(['Главная', 'Страница не найдена'])
				.not-found__wrapper.container
					.not-found__image
						img(src="./assets/img/not-found.jpg" alt="")/
					h1.not-found__title 404
						span ошибка
					p.not-found__desc Извините, но такой страницы нет на нашем сайте. Возможно вы ввели неправилный адрес или страница была удалена. Попробуйте вернутся на главную.
					a.btn._black(href="index.html") Перейти на главную
		include includes/footer.pug
		include includes/scripts.pug