style.
	.preloader {
		position: fixed;
		width: 100%;
		height: 100%;
		background: white;
		z-index: 9999;
	}

	.preloader._hidden {
		opacity: 0;
		visibility: hidden;
		-webkit-transition: 800ms;
		transition: 800ms;
	}

	.preloader__wrapper {
		position: absolute;
		top: 50%;
		left: 50%;
		-webkit-transform: translate(-50%, -50%);
		transform: translate(-50%, -50%);
		width: 200px;
		height: 200px;
	}

	.preloader__line {
		position: absolute;
		top: 0;
		left: 0;
		width: 100%;
		height: 100%;
		border-radius: 50%;
	}

	.preloader__line._one {
		border-bottom: 3px solid #d3d3d3;
		-webkit-animation: cssload-rotate-one 1.15s linear infinite;
		animation: cssload-rotate-one 1.15s linear infinite;
	}

	.preloader__line._two {
		border-bottom: 3px solid #dfba8e;
		-webkit-animation: cssload-rotate-two 1.15s linear infinite;
		animation: cssload-rotate-two 1.15s linear infinite;
	}

	.preloader__line._three {
		border-bottom: 3px solid #000000;
		-webkit-animation: cssload-rotate-three 1.15s linear infinite;
		animation: cssload-rotate-three 1.15s linear infinite;
	}

	@-webkit-keyframes cssload-rotate-one {
		0% {
			-webkit-transform: rotateX(35deg) rotateY(-45deg) rotateZ(0deg);
			transform: rotateX(35deg) rotateY(-45deg) rotateZ(0deg);
		}
		100% {
			-webkit-transform: rotateX(35deg) rotateY(-45deg) rotateZ(360deg);
			transform: rotateX(35deg) rotateY(-45deg) rotateZ(360deg);
		}
	}

	@keyframes cssload-rotate-one {
		0% {
			-webkit-transform: rotateX(35deg) rotateY(-45deg) rotateZ(0deg);
			transform: rotateX(35deg) rotateY(-45deg) rotateZ(0deg);
		}
		100% {
			-webkit-transform: rotateX(35deg) rotateY(-45deg) rotateZ(360deg);
			transform: rotateX(35deg) rotateY(-45deg) rotateZ(360deg);
		}
	}

	@-webkit-keyframes cssload-rotate-two {
		0% {
			-webkit-transform: rotateX(35deg) rotateY(45deg) rotateZ(360deg);
			transform: rotateX(35deg) rotateY(45deg) rotateZ(360deg);
		}
		100% {
			-webkit-transform: rotateX(35deg) rotateY(45deg) rotateZ(0deg);
			transform: rotateX(35deg) rotateY(45deg) rotateZ(0deg);
		}
	}

	@keyframes cssload-rotate-two {
		0% {
			-webkit-transform: rotateX(35deg) rotateY(45deg) rotateZ(360deg);
			transform: rotateX(35deg) rotateY(45deg) rotateZ(360deg);
		}
		100% {
			-webkit-transform: rotateX(35deg) rotateY(45deg) rotateZ(0deg);
			transform: rotateX(35deg) rotateY(45deg) rotateZ(0deg);
		}
	}

	@-webkit-keyframes cssload-rotate-three {
		0% {
			-webkit-transform: translateY(25%) rotateX(60deg) rotateY(180deg) rotateZ(360deg);
			transform: translateY(25%) rotateX(60deg) rotateY(180deg) rotateZ(360deg);
		}
		100% {
			-webkit-transform: translateY(25%) rotateX(60deg) rotateY(180deg) rotateZ(0deg);
			transform: translateY(25%) rotateX(60deg) rotateY(180deg) rotateZ(0deg);
		}
	}

	@keyframes cssload-rotate-three {
		0% {
			-webkit-transform: translateY(25%) rotateX(60deg) rotateY(180deg) rotateZ(360deg);
			transform: translateY(25%) rotateX(60deg) rotateY(180deg) rotateZ(360deg);
		}
		100% {
			-webkit-transform: translateY(25%) rotateX(60deg) rotateY(180deg) rotateZ(0deg);
			transform: translateY(25%) rotateX(60deg) rotateY(180deg) rotateZ(0deg);
		}
	}
